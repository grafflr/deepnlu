    "import spacy\n",
    "\n",
    "nlp = spacy.load(\"en_core_web_sm\")\n",
    "doc = nlp(\"clinical trials have clinical outcomes in clinical times\")\n",
    "for token in doc:\n",
    "    print(f\"{token.i}#{token.idx}#{token.orth}\", f\"{token.head.i}#{token.head.idx}#{hash(token.head.orth)}\")"
