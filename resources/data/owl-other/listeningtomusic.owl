                              ] .



###  https://w3id.org/MON/listeningtomusic.owl#ListeningToMusic

:ListeningToMusic rdf:type owl:Class ;
                  
                  rdfs:label "Listening to music"@en ;
                  
                  rdfs:subClassOf <https://w3id.org/MON/activity.owl#Activity> ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :includes ;
                                    owl:allValuesFrom :ListenToSong
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :includes ;
                                    owl:allValuesFrom :SkipSong
                                  ] ;
                  
                  rdfs:comment "The activity of listening to some music."@en .



###  https://w3id.org/MON/listeningtomusic.owl#SkipSong

:SkipSong rdf:type owl:Class ;
          
          rdfs:label "Skip song"@en ;
          
          rdfs:subClassOf <https://w3id.org/MON/action.owl#Action> ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <https://w3id.org/MON/music.owl#hasSong> ;
                            owl:someValuesFrom <https://w3id.org/MON/music.owl#Song>
                          ] .



