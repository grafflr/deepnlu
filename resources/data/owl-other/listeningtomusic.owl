###  https://w3id.org/MON/listeningtomusic.owl#ListenToSong

:ListenToSong rdf:type owl:Class ;
              
              rdfs:label "Listen to song"@en ;
              
              rdfs:subClassOf <https://w3id.org/MON/action.owl#Action> ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty <https://w3id.org/MON/music.owl#hasSong> ;
