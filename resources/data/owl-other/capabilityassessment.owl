###  https://w3id.org/MON/capabilityassessment.owl#Capability

:Capability rdf:type owl:Class ;
            
            rdfs:label "Capability"@en ;
            
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty :isAssessedBy ;
                              owl:allValuesFrom <https://w3id.org/MON/cga.owl#Question>
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <https://w3id.org/MON/generic.owl#description> ;
                              owl:allValuesFrom rdfs:Literal
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <https://w3id.org/MON/generic.owl#name> ;
                              owl:someValuesFrom rdfs:Literal
                            ] ;
            
            rdfs:comment "The power or ability to do something"@en .



###  https://w3id.org/MON/capabilityassessment.owl#CapabilityAssessment

:CapabilityAssessment rdf:type owl:Class ;
                      
                      rdfs:label "Capability assessment"@en ;
                      
                      rdfs:subClassOf <https://w3id.org/MON/cga.owl#GeriatricAssessment> ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :hasCapabilityLevel ;
                                        owl:someValuesFrom :CapabilityLevel
                                      ] ;
                      
                      owl:disjointWith :IndexAssessment ;
                      
                      rdfs:comment "An action with the aim of assessing the level of some capability of a certain patient at a certain time by performing a certain test."@en .



###  https://w3id.org/MON/capabilityassessment.owl#CapabilityLevel

:CapabilityLevel rdf:type owl:Class ;
                 
                 rdfs:label "Capability level"@en ;
                 
                 rdfs:subClassOf [ rdf:type owl:Restriction ;
                                   owl:onProperty <https://w3id.org/MON/generic.owl#name> ;
                                   owl:someValuesFrom rdfs:Literal
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :hasCapability ;
                                   owl:someValuesFrom :Capability
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty <https://w3id.org/MON/generic.owl#description> ;
                                   owl:someValuesFrom rdfs:Literal
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :isAssessedBy ;
                                   owl:allValuesFrom <https://w3id.org/MON/cga.owl#Question>
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty <https://w3id.org/MON/cga.owl#score> ;
                                   owl:someValuesFrom rdfs:Literal
                                 ] ;
                 
                 rdfs:comment "The level of a certain capability intended as the quality of a performance in doing something."@en .



###  https://w3id.org/MON/capabilityassessment.owl#IndexAssessment

:IndexAssessment rdf:type owl:Class ;
                 
                 rdfs:label "Index assessment"@en ;
                 
                 rdfs:subClassOf <https://w3id.org/MON/cga.owl#GeriatricAssessment> ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty <https://w3id.org/MON/clinicalact.owl#hasMember> ;
                                   owl:someValuesFrom :CapabilityAssessment
                                 ] ;
                 
                 rdfs:comment "The action of assessing a geriatric index."@en .



