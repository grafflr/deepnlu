###  https://w3id.org/MON/action.owl#byAgent

:byAgent rdf:type owl:ObjectProperty ;
         
         rdfs:label "by agent"@en ;
         
         rdfs:comment "The Agent who performs the Action."@en ;
         
         rdfs:domain :Action ;
         
         rdfs:range :Agent ;
         
         rdfs:subPropertyOf :hasParticipant .



###  https://w3id.org/MON/action.owl#executesTask

:executesTask rdf:type owl:ObjectProperty ;
              
              rdfs:label "executes task"@en ;
              
              rdfs:comment "A relation between an action and the executed task, e.g. 'putting some water in a pot and putting the pot on a fire until the water starts bubbling' executes the task 'boiling'."@en ;
              
              rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#executesTask> ;
              
              rdfs:domain :Action ;
              
              rdfs:range :Task .



###  https://w3id.org/MON/action.owl#hasParameter

:hasParameter rdf:type owl:ObjectProperty ;
              
              rdfs:label "has parameter"@en ;
              
              rdfs:comment "It associates the Task with its parameters."@en ;
              
              rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith> ;
              
              rdfs:domain :Behavior ;
              
              rdfs:range :BehaviorParameter .



###  https://w3id.org/MON/action.owl#hasParticipant

:hasParticipant rdf:type owl:ObjectProperty ;
                
                rdfs:label "has participant"@en ;
                
                rdfs:comment "The participant to a certain Action."@en ;
                
                rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasParticipant> .



###  https://w3id.org/MON/action.owl#hasPatient

:hasPatient rdf:type owl:ObjectProperty ;
            
            rdfs:label "has patient"@en ;
            
            rdfs:comment "It denotes something that is affected or acted upon by the action."@en ;
            
            rdfs:subPropertyOf :hasParticipant .





