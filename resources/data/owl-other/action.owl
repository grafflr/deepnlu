###  https://w3id.org/MON/action.owl#Action

:Action rdf:type owl:Class ;
        
        rdfs:label "Action"@en ;
        
        rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Action> ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty <https://w3id.org/MON/time.owl#atTime> ;
                          owl:someValuesFrom <https://w3id.org/MON/time.owl#TemporalEntity>
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :executesTask ;
                          owl:someValuesFrom :Task
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :byAgent ;
                          owl:someValuesFrom :Agent
                        ] ;
        
        owl:disjointWith :Agent ,
                         :Task ,
                         <https://w3id.org/MON/time.owl#TemporalEntity> ;
        
        rdfs:comment "The fact or process of doing something, typically to achieve an aim."@en .



###  https://w3id.org/MON/action.owl#Agent

:Agent rdf:type owl:Class ;
       
       rdfs:label "Agent"@en ;
       
       rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent> ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty <https://w3id.org/MON/generic.owl#name> ;
                         owl:someValuesFrom xsd:string
                       ] ;
       
       owl:disjointWith :Task ,
                        <https://w3id.org/MON/time.owl#TemporalEntity> ;
       
       rdfs:comment "Any agentive Object , either physical (e.g. a whale, a robot, an oak), or social (e.g. a corporation, an institution, a community)."@en .



###  https://w3id.org/MON/action.owl#Behavior

:Behavior rdf:type owl:Class ;
          
          rdfs:label "Behavior"@en ;
          
          rdfs:subClassOf :Task ;
          
          rdfs:comment "A Behavior is a task, implemented in as software artifact, that aims to realize a specific goal."@en .



###  https://w3id.org/MON/action.owl#BehaviorParameter

:BehaviorParameter rdf:type owl:Class ;
                   
                   rdfs:label "Behavior parameter"@en ;
                   
                   rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Parameter> ;
                   
                   rdfs:comment "A parameter of a behavior."@en .



###  https://w3id.org/MON/action.owl#Task

:Task rdf:type owl:Class ;
      
      rdfs:label "Task"@en ;
      
      rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Task> ;
      
      owl:disjointWith <https://w3id.org/MON/time.owl#TemporalEntity> ;
      
      rdfs:comment "Any EventType that aims at releasing a goal."@en .



