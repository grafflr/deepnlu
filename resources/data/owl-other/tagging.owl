###  https://w3id.org/MON/tagging.owl#appearsIn

:appearsIn rdf:type owl:ObjectProperty ;
           
           rdfs:label "appears in"@en ;
           
           rdfs:comment "It is used to connect the Person with the Media s/he appears in."@en ;
           
           rdfs:range <https://w3id.org/MON/multimediacontent.owl#Media> ;
           
           rdfs:domain <https://w3id.org/MON/person.owl#Person> ;
           
           owl:propertyChainAxiom ( [ owl:inverseOf :taggedPerson
                                    ]
                                    :forEntity
                                  ) .



###  https://w3id.org/MON/tagging.owl#forEntity

:forEntity rdf:type owl:ObjectProperty ;
           
           rdfs:label "for entity"@en ;
           
           rdfs:comment "It associates the Tagging action with the tagged Entity."@en ;
           
           rdfs:domain :Tagging ;
           
           owl:propertyChainAxiom ( :forEntity
                                    [ owl:inverseOf <https://w3id.org/MON/event.owl#hasEvent>
                                    ]
                                  ) .



###  https://w3id.org/MON/tagging.owl#hasTaggedPortion

:hasTaggedPortion rdf:type owl:FunctionalProperty ,
                           owl:ObjectProperty ;
                  
                  rdfs:label "has tagged portion"@en ;
                  
                  rdfs:domain :ImageTagging ;
                  
                  rdfs:range :TaggedPortion .



###  https://w3id.org/MON/tagging.owl#taggedPerson

:taggedPerson rdf:type owl:ObjectProperty ;
              
              rdfs:label "tagged person"@en ;
              
              rdfs:range <https://w3id.org/MON/person.owl#Person> ;
              
              rdfs:subPropertyOf :usingTag .



###  https://w3id.org/MON/tagging.owl#thumbnail

:thumbnail rdf:type owl:ObjectProperty ;
           
           rdfs:label "thumbnail"@en ;
           
           rdfs:comment "A small picture of a Person"@en ;
           
           rdfs:range <https://w3id.org/MON/multimediacontent.owl#Image> ;
           
           rdfs:subPropertyOf :appearsIn .



###  https://w3id.org/MON/tagging.owl#usingTag

:usingTag rdf:type owl:ObjectProperty ;
          
          rdfs:label "using tag"@en ;
          
          rdfs:comment "It associates the Tagging action with the Tag."@en ;
          
          rdfs:range :Tag ;
          
          rdfs:domain :Tagging .





