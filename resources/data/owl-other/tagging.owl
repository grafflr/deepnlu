###  https://w3id.org/MON/tagging.owl#height

:height rdf:type owl:DatatypeProperty ,
                 owl:FunctionalProperty ;
        
        rdfs:label "height"@en ;
        
        rdfs:comment "The height (in pixels) of the tagged portion rectangle."@en ;
        
        rdfs:range xsd:integer ;
        
        rdfs:domain :TaggedPortion .



###  https://w3id.org/MON/tagging.owl#label

:label rdf:type owl:DatatypeProperty ,
                owl:FunctionalProperty ;
       
       rdfs:label "label"@en ;
       
       rdfs:comment "It encodes the label of a Tag."@en ;
       
       rdfs:range rdfs:Literal ;
       
       rdfs:domain :Tag .



###  https://w3id.org/MON/tagging.owl#left

:left rdf:type owl:DatatypeProperty ,
               owl:FunctionalProperty ;
      
      rdfs:label "left"@en ;
      
      rdfs:comment "The distance (in pixels) between the top left hand side corner of the tagged portion and the left hand side of the picture."@en ;
      
      rdfs:range xsd:integer ;
      
      rdfs:domain :TaggedPortion .



###  https://w3id.org/MON/tagging.owl#top

:top rdf:type owl:DatatypeProperty ,
              owl:FunctionalProperty ;
     
     rdfs:label "top"@en ;
     
     rdfs:comment "The distance (in pixels) between the top left hand side corner of the tagged portion and the top side of the picture."@en ;
     
     rdfs:range xsd:integer ;
     
     rdfs:domain :TaggedPortion .



###  https://w3id.org/MON/tagging.owl#width

:width rdf:type owl:DatatypeProperty ,
                owl:FunctionalProperty ;
       
       rdfs:label "width"@en ;
       
       rdfs:comment "The width (in pixels) of the tagged portion rectangle."@en ;
       
       rdfs:range xsd:integer ;
       
       rdfs:domain :TaggedPortion .





