<https://w3id.org/MON/spatial.owl> rdf:type owl:Ontology ;
                                   
                                   dc:title "Spatial Ontology"@en ;
                                   
                                   dc:description """The MON's Spatial  provides classes and properties for specifying the knowledge about spatial things (i.e. anything with a spatial extent) and places (e.g. cities).
These concepts are used for specifying the generic locations for physical objects.
This ontology module is an implementation of the  <a href=\"http://ontologydesignpatterns.org/wiki/Submissions:Place\">Ontology Design Pattern Place</a>."""@en ,
                                                  <https://raw.githubusercontent.com/luigi-asprino/MON/main/src/Spatial.png> ;
                                   
                                   owl:imports <https://w3id.org/MON/action.owl> .


#################################################################
#
#    Object Properties
#
#################################################################


###  https://w3id.org/MON/spatial.owl#hasPlace

:hasPlace rdf:type owl:ObjectProperty ;
          
          rdfs:label "has place"@en ;
          
          rdfs:range :SpatialThing .



###  https://w3id.org/MON/spatial.owl#hasResidence

:hasResidence rdf:type owl:ObjectProperty ;
              
              rdfs:label "has residence"@en ;
              
              rdfs:domain <https://w3id.org/MON/action.owl#Agent> ;
              
              rdfs:range :Residence ;
              
              rdfs:subPropertyOf :hasPlace .





#################################################################
#
#    Classes
#
#################################################################


###  https://w3id.org/MON/action.owl#Action

<https://w3id.org/MON/action.owl#Action> owl:disjointWith <https://w3id.org/MON/action.owl#Agent> ,
                                                          <https://w3id.org/MON/action.owl#Task> ,
                                                          :SpatialThing ,
                                                          <https://w3id.org/MON/time.owl#TemporalEntity> .



###  https://w3id.org/MON/action.owl#Agent

<https://w3id.org/MON/action.owl#Agent> rdf:type owl:Class ;
                                        
                                        owl:disjointWith <https://w3id.org/MON/action.owl#Task> ,
                                                         :SpatialThing ,
                                                         <https://w3id.org/MON/time.owl#TemporalEntity> .



###  https://w3id.org/MON/action.owl#Task

<https://w3id.org/MON/action.owl#Task> owl:disjointWith :SpatialThing ,
                                                        <https://w3id.org/MON/time.owl#TemporalEntity> .



###  https://w3id.org/MON/spatial.owl#City

:City rdf:type owl:Class ;
      
      rdfs:label "City"@en ;
      
      rdfs:subClassOf :SpatialThing .



###  https://w3id.org/MON/spatial.owl#Residence

:Residence rdf:type owl:Class ;
           
           rdfs:label "Residence"@en ;
           
           rdfs:subClassOf :SpatialThing .



###  https://w3id.org/MON/spatial.owl#SpatialThing

:SpatialThing rdf:type owl:Class ;
              
              rdfs:label "Spatial thing"@en ;
              
              rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject> ;
              
              owl:disjointWith <https://w3id.org/MON/time.owl#TemporalEntity> ;
              
              rdfs:comment "Anything with spatial extent, i.e. size, shape, or position."@en .



