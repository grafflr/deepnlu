@prefix : <https://w3id.org/MON/music.owl#> .
@prefix j.0: <http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <https://w3id.org/MON/music.owl> .

<https://w3id.org/MON/music.owl> rdf:type owl:Ontology ;
                                 
                                 j.0:hasIntent "To represent concepts and properties for describing music that can be found in personal music players."@en ;
                                 
                                 owl:imports <https://w3id.org/MON/generic.owl> ,
                                             <https://w3id.org/MON/multimediacontent.owl> ,
                                             <https://w3id.org/MON/person.owl> .


#################################################################
#
#    Object Properties
#
#################################################################


###  https://w3id.org/MON/music.owl#directlyFollows

:directlyFollows rdf:type owl:FunctionalProperty ,
                          owl:ObjectProperty ;
                 
                 rdfs:label "directly follows"@en ;
                 
                 rdfs:range :PlaylistSlot ;
                 
                 rdfs:domain :PlaylistSlot .



###  https://w3id.org/MON/music.owl#directlyPrecedes

:directlyPrecedes rdf:type owl:FunctionalProperty ,
                           owl:ObjectProperty ;
                  
                  rdfs:label "directly precedes"@en ;
                  
                  rdfs:domain :PlaylistSlot ;
                  
                  rdfs:range :PlaylistSlot .



###  https://w3id.org/MON/music.owl#hasAuthor

:hasAuthor rdf:type owl:ObjectProperty ;
           
           rdfs:label "has author"@en ;
           
           rdfs:comment "The creator of the musical work."@en ;
           
           rdfs:domain :MusicalWork ;
           
           rdfs:range [ rdf:type owl:Class ;
                        owl:unionOf ( :MusicArtist
                                      :MusicGroup
                                    )
                      ] .



###  https://w3id.org/MON/music.owl#hasGenre

:hasGenre rdf:type owl:ObjectProperty ;
          
          rdfs:label "has genre" ;
          
          rdfs:comment "It associates something (e.g. a Playlist, a Song) with the genre to which it belongs to."@en ;
          
          rdfs:range :Genre ;
          
          rdfs:domain :MusicalWork .



###  https://w3id.org/MON/music.owl#hasMusicVideo

:hasMusicVideo rdf:type owl:ObjectProperty ;
               
               rdfs:label "has music video"@en ;
               
               rdfs:comment "It associates the song with the video file encoding the video produced for the promoting the song or for artistic purposes."@en ;
               
               rdfs:range <https://w3id.org/MON/multimediacontent.owl#Video> ;
               
               rdfs:domain :Song .



###  https://w3id.org/MON/music.owl#hasMusicalWork

:hasMusicalWork rdf:type owl:ObjectProperty ;
                
                rdfs:label "has musical work"@en ;
                
                rdfs:comment "A relation between a 'MusicalWork' and another entity which could be a container (i.e. Playlist) or an event (i.e. the user is listening to a certain song)."@en ;
                
                rdfs:range :MusicalWork .



###  https://w3id.org/MON/music.owl#hasSlot

:hasSlot rdf:type owl:ObjectProperty ;
         
         rdfs:label "has slot"@en ;
         
         rdfs:comment "It associates a 'Playlist' with the PlaylistSlots composing it."@en ;
         
         rdfs:domain :Playlist ;
         
         rdfs:range :PlaylistSlot .



###  https://w3id.org/MON/music.owl#hasSong

:hasSong rdf:type owl:ObjectProperty ;
         
         rdfs:label "has song"@en ;
         
         rdfs:comment "A relation between a 'Song' and another entity which could be a container (i.e. Playlist) or an event (i.e. the user is listening to a certain song)."@en ;
         
         rdfs:range :Song ;
         
         rdfs:subPropertyOf :hasMusicalWork .



###  https://w3id.org/MON/music.owl#isRecordedIn

:isRecordedIn rdf:type owl:ObjectProperty ;
              
              rdfs:label "is recorded in"@en ;
              
              rdfs:comment "It associates a MusicalWork with the AudioFile in whith it is recorded in."@en ;
              
              rdfs:range <https://w3id.org/MON/multimediacontent.owl#Audio> ;
              
              rdfs:domain :MusicalWork .



###  https://w3id.org/MON/music.owl/hasFirstSlot

<https://w3id.org/MON/music.owl/hasFirstSlot> rdf:type owl:ObjectProperty ;
                                              
                                              rdfs:label "has first slot"@en ;
                                              
                                              rdfs:comment "The first slot of the playlist."@en ;
                                              
                                              rdfs:subPropertyOf :hasSlot .





#################################################################
#
#    Data properties
#
#################################################################


###  https://w3id.org/MON/music.owl#index

:index rdf:type owl:DatatypeProperty ,
                owl:FunctionalProperty ;
       
       rdfs:label "index"@en ;
       
       rdfs:comment "It encodes the index of a 'PlaylistSlot'."@en ;
       
       rdfs:range xsd:integer ;
       
       rdfs:domain :PlaylistSlot .





#################################################################
#
#    Classes
#
#################################################################


###  https://w3id.org/MON/music.owl#Album

:Album rdf:type owl:Class ;
       
       rdfs:label "Album"@en ;
       
       rdfs:subClassOf :MusicalCollection ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :hasSong ;
                         owl:someValuesFrom :Song
                       ] ;
       
       owl:disjointWith [ rdf:type owl:Class ;
                          owl:unionOf ( :Discography
                                        :Song
                                      )
                        ] ;
       
       rdfs:comment "One or more recordings issued together."@en .



###  https://w3id.org/MON/music.owl#Discography

:Discography rdf:type owl:Class ;
             
             rdfs:label "Discography"@en ;
             
             rdfs:subClassOf :MusicalCollection ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :hasMusicalWork ;
                               owl:someValuesFrom [ rdf:type owl:Class ;
                                                    owl:unionOf ( :Album
                                                                  :Song
                                                                )
                                                  ]
                             ] ;
             
             owl:disjointWith [ rdf:type owl:Class ;
                                owl:unionOf ( :Album
                                              :Song
                                            )
                              ] ;
             
             rdfs:comment "A descriptive catalog of musical recordings recordered by an author."@en .



###  https://w3id.org/MON/music.owl#Genre

:Genre rdf:type owl:Class ;
       
       rdfs:label "Genre"@en ;
       
       rdfs:subClassOf [ rdf:type owl:Restriction ;
                         owl:onProperty <https://w3id.org/MON/generic.owl#name> ;
                         owl:someValuesFrom xsd:string
                       ] ;
       
       rdfs:comment "An expressive style of music."@en .



###  https://w3id.org/MON/music.owl#MusicArtist

:MusicArtist rdf:type owl:Class ;
             
             rdfs:label "Music Artist"@en ;
             
             rdfs:subClassOf <https://w3id.org/MON/person.owl#Person> ;
             
             rdfs:comment "A music artist is a musician and/or singer who records and releases music."@en .



###  https://w3id.org/MON/music.owl#MusicGroup

:MusicGroup rdf:type owl:Class ;
            
            rdfs:label "Music Group"@en ;
            
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty <https://w3id.org/MON/generic.owl#name> ;
                              owl:someValuesFrom xsd:string
                            ] ;
            
            rdfs:comment "Group of musicians, or musical ensemble, usually popular or folk, playing parts of or improvising off of a musical arrangement."@en .



###  https://w3id.org/MON/music.owl#MusicalCollection

:MusicalCollection rdf:type owl:Class ;
                   
                   rdfs:label "Musical Collection"@en ;
                   
                   rdfs:subClassOf :MusicalWork ;
                   
                   rdfs:comment "Any Musical Collection."@en .



###  https://w3id.org/MON/music.owl#MusicalWork

:MusicalWork rdf:type owl:Class ;
             
             rdfs:label "Musical Work"@en ;
             
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty :hasGenre ;
                               owl:someValuesFrom :Genre
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty <https://w3id.org/MON/generic.owl#name> ;
                               owl:someValuesFrom xsd:string
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :isRecordedIn ;
                               owl:someValuesFrom <https://w3id.org/MON/multimediacontent.owl#Audio>
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :hasAuthor ;
                               owl:someValuesFrom [ rdf:type owl:Class ;
                                                    owl:unionOf ( :MusicArtist
                                                                  :MusicGroup
                                                                )
                                                  ]
                             ] ;
             
             rdfs:comment "Distinct intellectual or artistic musical creation."@en .



###  https://w3id.org/MON/music.owl#Playlist

:Playlist rdf:type owl:Class ;
          
          rdfs:label "Playlist"@en ;
          
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty :hasSlot ;
                            owl:someValuesFrom :PlaylistSlot
                          ] ;
          
          rdfs:comment "A playlist of 'Musical work'."@en .



###  https://w3id.org/MON/music.owl#PlaylistSlot

:PlaylistSlot rdf:type owl:Class ;
              
              rdfs:label "Playlist Slot"@en ;
              
              rdfs:subClassOf [ rdf:type owl:Restriction ;
                                owl:onProperty :index ;
                                owl:someValuesFrom xsd:integer
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :directlyPrecedes ;
                                owl:allValuesFrom :PlaylistSlot
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :directlyFollows ;
                                owl:allValuesFrom :PlaylistSlot
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasSong ;
                                owl:someValuesFrom :Song
                              ] ;
              
              rdfs:comment "A slot of a playlist."@en .



###  https://w3id.org/MON/music.owl#Song

:Song rdf:type owl:Class ;
      
      rdfs:label "Song"@en ;
      
      rdfs:subClassOf :MusicalWork ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasMusicVideo ;
                        owl:someValuesFrom <https://w3id.org/MON/multimediacontent.owl#Video>
                      ] ;
      
      owl:disjointWith [ rdf:type owl:Class ;
                         owl:unionOf ( :Album
                                       :Discography
                                     )
                       ] ;
      
      rdfs:comment "A short musical composition with words."@en .




