###  https://w3id.org/MON/cirs.owl#comorbidityIndex

:comorbidityIndex rdf:type owl:DatatypeProperty ;
                  
                  rdfs:label "Comorbidity index (CIRS-CI)"@en ;
                  
                  rdfs:comment "The number of biological system with a score of 3 or greater (excluded the psychiatric one)."@en ;
                  
                  rdfs:range xsd:integer ;
                  
                  rdfs:domain :CIRSAssessment .



###  https://w3id.org/MON/cirs.owl#illnessSeverityScore

:illnessSeverityScore rdf:type owl:DatatypeProperty ;
                      
                      rdfs:label "Illness severity score (CIRS-IS)"@en ;
                      
                      rdfs:comment "The mean of all single biological system assessment (excluded the psychiatric one) of a CIRS evalutation."@en ;
                      
                      rdfs:range xsd:double ;
                      
                      rdfs:domain :CIRSAssessment .



