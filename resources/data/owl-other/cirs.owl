                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty :assessesBiologicalSystem ;
                                              owl:someValuesFrom :BiologicalSystem
                                            ] ;
                            
                            rdfs:comment "The action of assessing the illness rating of the biological system of a patient."@en .



###  https://w3id.org/MON/cirs.owl#CIRSAssessment

:CIRSAssessment rdf:type owl:Class ;
                
                rdfs:label "CIRS Assessment"@en ;
                
                rdfs:subClassOf <https://w3id.org/MON/cga.owl#GeriatricAssessment> ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :illnessSeverityScore ;
                                  owl:someValuesFrom xsd:double
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <https://w3id.org/MON/clinicalact.owl#hasMember> ;
                                  owl:someValuesFrom :BiologicalSystemAssessment
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :comorbidityIndex ;
                                  owl:someValuesFrom xsd:integer
                                ] ;
                
                rdfs:comment "The action of assessing the CIRS for a patient."@en .



###  https://w3id.org/MON/cirs.owl#CIRSRating

:CIRSRating rdf:type owl:Class ;
            
