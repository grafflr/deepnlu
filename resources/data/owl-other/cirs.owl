###  https://w3id.org/MON/cirs.owl#BiologicalSystem

:BiologicalSystem rdf:type owl:Class ;
                  
                  rdfs:label "Biological system"@en ;
                  
                  rdfs:subClassOf [ rdf:type owl:Restriction ;
                                    owl:onProperty <https://w3id.org/MON/generic.owl#description> ;
                                    owl:someValuesFrom rdfs:Literal
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty <https://w3id.org/MON/generic.owl#name> ;
                                    owl:someValuesFrom rdfs:Literal
                                  ] ;
                  
                  rdfs:comment "A complex network of biologically relevant entities (e.g. respiratory system, vascular system)."@en .



###  https://w3id.org/MON/cirs.owl#BiologicalSystemAssessment

:BiologicalSystemAssessment rdf:type owl:Class ;
                            
                            rdfs:label "Biological system assessment"@en ;
                            
                            rdfs:subClassOf <https://w3id.org/MON/cga.owl#GeriatricAssessment> ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty :hasRating ;
                                              owl:someValuesFrom :CIRSRating
