###  https://w3id.org/MON/cga.owl#GeriatricAssessment

<https://w3id.org/MON/cga.owl#GeriatricAssessment> rdf:type owl:Class .



###  https://w3id.org/MON/cga.owl#Question

<https://w3id.org/MON/cga.owl#Question> rdf:type owl:Class .



###  https://w3id.org/MON/ess.owl#Activity

:Activity rdf:type owl:Class ;
          
          rdfs:label "Activity"@en ;
          
          rdfs:subClassOf :PatientCondition ;
          
          rdfs:comment "The Activity (e.g. in bed all day, chairfast etc.) of a patient as intended in the Exton-Smith Scale."@en .



###  https://w3id.org/MON/ess.owl#Exton-SmithScaleAssessment

:Exton-SmithScaleAssessment rdf:type owl:Class ;
                            
                            rdfs:label "Exton-Smith Scale (ESS) Assessment"@en ;
                            
                            rdfs:subClassOf <https://w3id.org/MON/cga.owl#GeriatricAssessment> ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty <https://w3id.org/MON/cga.owl#score> ;
                                              owl:someValuesFrom rdfs:Literal
                                            ] ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty :hasCondition ;
                                              owl:someValuesFrom :Incontinence
                                            ] ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty :hasCondition ;
                                              owl:someValuesFrom :MentalState
                                            ] ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty :hasCondition ;
                                              owl:someValuesFrom :MobilityInBed
                                            ] ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty :hasPressureSoresRisk ;
                                              owl:someValuesFrom :PressureSoresRisk
                                            ] ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty :hasCondition ;
                                              owl:someValuesFrom :Activity
                                            ] ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty :hasCondition ;
                                              owl:someValuesFrom :PatientGeneralCondition
                                            ] ;
                            
                            rdfs:comment "An action aiming at assessing the Exton-Smith Scale for a patient."@en .



###  https://w3id.org/MON/ess.owl#Incontinence

:Incontinence rdf:type owl:Class ;
              
              rdfs:label "Incontinence"@en ;
              
              rdfs:subClassOf :PatientCondition ;
              
              rdfs:comment "The incontinence (e.g. doubly incontinent, usually of urine etc.) of a patient as intended in the Exton-Smith Scale."@en .



###  https://w3id.org/MON/ess.owl#MentalState

:MentalState rdf:type owl:Class ;
             
             rdfs:label "Mental state"@en ;
             
             rdfs:subClassOf :MobilityInBed ;
             
             rdfs:comment "The Mental State (e.g. confused, alert etc.) of a patient as intended in the Exton-Smith Scale."@en .



###  https://w3id.org/MON/ess.owl#MobilityInBed

:MobilityInBed rdf:type owl:Class ;
               
               rdfs:label "Mobility in bed"@en ;
               
               rdfs:subClassOf :PatientCondition ;
               
               rdfs:comment "The mobility in bed (e.g. immobile, very limited etc) of a patient as intended  in the Exton-Smith Scale."@en .



###  https://w3id.org/MON/ess.owl#PatientCondition

:PatientCondition rdf:type owl:Class ;
                  
                  rdfs:label "Patient condition"@en ;
                  
                  rdfs:subClassOf [ rdf:type owl:Restriction ;
                                    owl:onProperty <https://w3id.org/MON/generic.owl#name> ;
                                    owl:someValuesFrom rdfs:Literal
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty <https://w3id.org/MON/cga.owl#score> ;
                                    owl:someValuesFrom rdfs:Literal
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty <https://w3id.org/MON/cga.owl#isAssessedBy> ;
                                    owl:allValuesFrom [ rdf:type owl:Class ;
                                                        owl:unionOf ( <https://w3id.org/MON/cga.owl#ClinicalTest>
                                                                      <https://w3id.org/MON/cga.owl#Question>
                                                                    )
                                                      ]
                                  ] .



###  https://w3id.org/MON/ess.owl#PatientGeneralCondition

:PatientGeneralCondition rdf:type owl:Class ;
                         
                         rdfs:label "Patient general condition"@en ;
                         
                         rdfs:subClassOf :PatientCondition ;
                         
                         rdfs:comment "The general condition (e.g. bad, good, fair etc) of a patient as intended  in the Exton-Smith Scale."@en .



###  https://w3id.org/MON/ess.owl#PressureSoresRisk

:PressureSoresRisk rdf:type owl:Class ;
                   
                   rdfs:label "Pressure sores risk"@en ;
                   
                   rdfs:subClassOf [ rdf:type owl:Restriction ;
                                     owl:onProperty <https://w3id.org/MON/cga.owl#scoreMax> ;
                                     owl:someValuesFrom rdfs:Literal
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty <https://w3id.org/MON/cga.owl#scoreMin> ;
                                     owl:someValuesFrom rdfs:Literal
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty <https://w3id.org/MON/generic.owl#name> ;
                                     owl:someValuesFrom rdfs:Literal
                                   ] ;
                   
                   rdfs:comment "A risk (e.g. high, medium etc.) of pressure sores."@en .




