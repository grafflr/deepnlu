@base <https://w3id.org/MON/pet.owl> .

<https://w3id.org/MON/pet.owl> rdf:type owl:Ontology ;
                               
                               dc:title "Pet Ontology"@en ;
                               
                               dc:description """The MON's Pet module is aimed at modeling the information related to pets.

<b>Example of Usage</b>

We provide an example of usage of the Pet ontology for specifying the following scenario.
Felix is a female cat.

<pre>
@prefix pet: &#60;https://w3id.org/MON/pet.owl&#62;
@prefix generic: &#60;https://w3id.org/MON/generic.owl#&#62;

:felix a pet:Pet ;
   pet:hasPetGender :female ;
   generic:name \"Felix\" .

:female a pet:PetGender ;
    generic:name \"Gender Female\" .

</pre>"""@en ,
                                              <https://raw.githubusercontent.com/luigi-asprino/MON/main/src/Pet.png> ;
                               
                               owl:imports <https://w3id.org/MON/action.owl> ,
                                           <https://w3id.org/MON/generic.owl> .


#################################################################
#
#    Object Properties
#
#################################################################


###  https://w3id.org/MON/pet.owl#hasPetGender

:hasPetGender rdf:type owl:ObjectProperty ;
              
              rdfs:label "has pet gender"@en ;
              
              rdfs:comment "It associates a Pet with its gender."@en ;
              
              rdfs:domain :Pet ;
              
              rdfs:range :PetGender .



###  https://w3id.org/MON/pet.owl#hasPetType

:hasPetType rdf:type owl:ObjectProperty ;
            
            rdfs:label "has pet type"@en ;
            
            rdfs:domain :Pet ;
            
            rdfs:range :PetType .





#################################################################
#
#    Classes
#
#################################################################


###  https://w3id.org/MON/action.owl#Action

<https://w3id.org/MON/action.owl#Action> owl:disjointWith <https://w3id.org/MON/action.owl#Agent> ,
                                                          <https://w3id.org/MON/action.owl#Task> ,
                                                          :PetGender ,
                                                          :PetType ,
                                                          <https://w3id.org/MON/time.owl#TemporalEntity> .



###  https://w3id.org/MON/action.owl#Agent

<https://w3id.org/MON/action.owl#Agent> owl:disjointWith <https://w3id.org/MON/action.owl#Task> ,
                                                         :PetGender ,
                                                         :PetType ,
                                                         <https://w3id.org/MON/time.owl#TemporalEntity> .



###  https://w3id.org/MON/action.owl#Task

<https://w3id.org/MON/action.owl#Task> owl:disjointWith :PetGender ,
                                                        :PetType ,
                                                        <https://w3id.org/MON/time.owl#TemporalEntity> .



###  https://w3id.org/MON/pet.owl#Pet

:Pet rdf:type owl:Class ;
     
     rdfs:label "Pet"@en ;
     
     rdfs:subClassOf <https://w3id.org/MON/action.owl#Agent> ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :hasPetType ;
                       owl:someValuesFrom :PetType
                     ] ;
     
     rdfs:comment "A domestic animal kept for companionship."@en .



###  https://w3id.org/MON/pet.owl#PetGender

:PetGender rdf:type owl:Class ;
           
           rdfs:label "Pet gender"@en ;
           
           rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Concept> ;
           
           owl:disjointWith :PetType ,
                            <https://w3id.org/MON/time.owl#TemporalEntity> ;
           
           rdfs:comment "The gender of a Pet which is used for verbalization purpose."@en .



###  https://w3id.org/MON/pet.owl#PetType

:PetType rdf:type owl:Class ;
         
         rdfs:label "Pet Type"@en ;
         
         rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Concept> ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty <https://w3id.org/MON/generic.owl#name> ;
                           owl:someValuesFrom xsd:string
                         ] ;
         
         owl:disjointWith <https://w3id.org/MON/time.owl#TemporalEntity> ;
         
         rdfs:comment "The type of a Pet (e.g. Cat, Dog etc)."@en .



