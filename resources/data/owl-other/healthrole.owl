###  https://w3id.org/MON/healthrole.owl#Nurse

:Nurse rdf:type owl:Class ;
       
       rdfs:label "Nurse"@en ;
       
       rdfs:subClassOf <https://w3id.org/MON/person.owl#Person> ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :hasHealthRole ;
                         owl:hasValue :NurseRole
