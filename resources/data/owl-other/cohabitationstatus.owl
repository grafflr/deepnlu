###  https://w3id.org/MON/cohabitationstatus.owl#Co-HabitationStatus

:Co-HabitationStatus rdf:type owl:Class ;
                     
                     rdfs:label "Co-Habitation status"@en ;
                     
                     rdfs:subClassOf <https://w3id.org/MON/timeindexedsituation.owl#TimeIndexedSituation> ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty <https://w3id.org/MON/timeindexedsituation.owl#forPatient> ;
                                       owl:someValuesFrom <https://w3id.org/MON/healthrole.owl#Patient>
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty <https://w3id.org/MON/timeindexedsituation.owl#forEntity> ;
                                       owl:allValuesFrom <https://w3id.org/MON/person.owl#Person>
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty <https://w3id.org/MON/spatial.owl#hasPlace> ;
                                       owl:someValuesFrom <https://w3id.org/MON/spatial.owl#Residence>
                                     ] ;
                     
                     rdfs:comment "A time indexed situation representing the co-habitation status for a patient at a certain time."@en .



###  https://w3id.org/MON/cohabitationstatus.owl#Co-HabitationStatusAssessment

:Co-HabitationStatusAssessment rdf:type owl:Class ;
                               
                               rdfs:label "Co-Habitation status assessment"@en ;
                               
                               rdfs:subClassOf <https://w3id.org/MON/cga.owl#GeriatricAssessment> ,
                                               [ rdf:type owl:Restriction ;
                                                 owl:onProperty <https://w3id.org/MON/cga.owl#score> ;
                                                 owl:someValuesFrom rdfs:Literal
                                               ] ,
                                               [ rdf:type owl:Restriction ;
                                                 owl:onProperty <https://w3id.org/MON/cga.owl#assessesSituation> ;
                                                 owl:someValuesFrom :Co-HabitationStatus
                                               ] ;
                               
                               rdfs:comment "An action with the aim of assessing the co-habitation status of a patient at a certain time."@en .



