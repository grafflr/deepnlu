###  https://w3id.org/MON/multimediacontent.owl#Audio

:Audio rdf:type owl:Class ;
       
       rdfs:label "Audio"@en ;
       
       rdfs:subClassOf :Media ;
       
       rdfs:comment "An Audio file."@en .



###  https://w3id.org/MON/multimediacontent.owl#Image

:Image rdf:type owl:Class ;
       
       rdfs:label "Image"@en ;
       
       rdfs:subClassOf :Media ;
       
       rdfs:comment "An Image file."@en .



###  https://w3id.org/MON/multimediacontent.owl#Media

:Media rdf:type owl:Class ;
       
       rdfs:label "Media"@en ;
       
       rdfs:subClassOf [ rdf:type owl:Restriction ;
                         owl:onProperty <https://w3id.org/MON/generic.owl#name> ;
                         owl:allValuesFrom xsd:string
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :url ;
                         owl:allValuesFrom xsd:anyURI
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty <https://w3id.org/MON/time.owl#atTime> ;
                         owl:allValuesFrom <https://w3id.org/MON/time.owl#TemporalEntity>
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty <https://w3id.org/MON/generic.owl#description> ;
                         owl:allValuesFrom xsd:string
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty <https://w3id.org/MON/event.owl#hasEvent> ;
                         owl:allValuesFrom <https://w3id.org/MON/event.owl#Event>
                       ] ;
       
       rdfs:comment "This class represents the collective communication outlets or tools that are used to store and deliver information or data, such as photos, videos, audios, digital documents etc."@en .



###  https://w3id.org/MON/multimediacontent.owl#Video

:Video rdf:type owl:Class ;
       
       rdfs:label "Video"@en ;
       
       rdfs:subClassOf :Media ;
       
       rdfs:comment "A video file."@en .




