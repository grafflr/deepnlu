###  https://w3id.org/MON/multimediacontent.owl#author

:author rdf:type owl:DatatypeProperty ;
        
        rdfs:label "author"@en ;
        
        rdfs:comment "The creator of the Media content."@en ;
        
        rdfs:range xsd:string ;
        
        rdfs:domain :Media .



###  https://w3id.org/MON/multimediacontent.owl#fileFormat

:fileFormat rdf:type owl:DatatypeProperty ,
                     owl:FunctionalProperty ;
            
            rdfs:label "file format"@en ;
            
            rdfs:comment "It encodes the file format of the Media content, i.e. png, mp4, mp3."@en ;
            
            rdfs:range xsd:string ;
            
            rdfs:domain :Media .



###  https://w3id.org/MON/multimediacontent.owl#url

:url rdf:type owl:DatatypeProperty ,
              owl:FunctionalProperty ;
     
     rdfs:comment "It encodes the location of the Media content."@en ;
     
     rdfs:range xsd:anyURI ;
     
     rdfs:domain :Media .





